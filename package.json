    "tymly",
    "package",
    "pg",
    "postgres"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/pg-diff-sync.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/pg-diff-sync/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "boom": "7.2.0",
    "debug": "3.1.0",
    "dottie": "2.0.0",
    "ejs": "2.6.1",
    "lodash": "4.17.10"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.3",

    "ejs": "3.1.9",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "chai": "4.3.10",
    "codecov": "3.8.3",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "semantic-release": "22.0.11",
    "standard": "17.1.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1"
  },
  "scripts": {
    "lint": "standard",
